{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "dev:admin": "mix --mix-config=webpack.admin.mix.js",
    "development": "mix",
    "watch": "mix watch",
    "watch:admin": "mix watch --mix-config=webpack.admin.mix.js",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "prod:admin": "mix --mix-config=webpack.admin.mix.js --production",
    "prod:all": "npm run prod && npm run prod:admin",
    "production": "mix --production"
  },
  "devDependencies": {
    "@popperjs/core": "^2.10.2",
    "axios": "^0.25",
    "bootstrap": "^5.2.0-beta1",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.32.6",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12",
    "craftable": "^2.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "aos": "^2.3.4",
    "lightgallery": "^2.4.0",
    "magic-grid": "^3.2.4",
    "simplebar": "^5.3.8",
    "swiper": "^8.2.2",
    "swiper-vue2": "^1.0.9"
  }
}
